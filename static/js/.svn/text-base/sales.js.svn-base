$(".orderBytype,.orderBysales").click(function () {
    var tbody = $(this).closest("table");
    if ($(this).find("em").hasClass("asc")) {
        sortByNum(this, tbody, 1,2);
    } else {
        sortByNum(this, tbody, 0,2);
    }
});

$('.business-sales-nav li.item').click(function () {
    $(this).addClass('active').siblings('li.item').removeClass('active');
})